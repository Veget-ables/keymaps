[
	{ "keys": ["alt+left"], "command": "next_view_in_stack" },
	{ "keys": ["alt+right"], "command": "prev_view_in_stack" },
	{ "keys": ["alt+ctrl+t"], "command": "reopen_last_file" },
	{ "keys": ["alt+t"], "command": "new_file" },
	{ "keys": ["alt+w"], "command": "close_file" },
	{ "keys": ["ctrl+w"], "command": "find_under_expand" },
	{ "keys": ["ctrl+y"], "command": "run_macro_file", "args": {"file": "res://Packages/Default/Delete Line.sublime-macro"} },
	{ "keys": ["ctrl+d"], "command": "duplicate_line" },
	{ "keys": ["alt+j"], "command": "find_under_expand" },
	{ "keys": ["ctrl+alt+enter"], "command": "run_macro_file", "args": {"file": "res://Packages/Default/Add Line Before.sublime-macro"} },


	{
		"keys": ["ctrl+alt+shift+s"],
		"command": "set_layout",
		"args":
		{
			"cols": [0.0, 1.0],
			"rows": [0.0, 1.0],
			"cells": [[0, 0, 1, 1]]
		}
	},

	{
		"keys": ["ctrl+alt+shift+v"],
		"command": "set_layout",
		"args":
		{
			"cols": [0.0, 0.5, 1.0],
			"rows": [0.0, 1.0],
			"cells": [[0, 0, 1, 1], [1, 0, 2, 1]]
		}
	},

	{
		"keys": ["ctrl+alt+shift+h"],
		"command": "set_layout",
		"args":
		{
			"cols": [0.0, 1.0],
			"rows": [0.0, 0.5, 1.0],
			"cells": [[0, 0, 1, 1], [0, 1, 1, 2]]
		}
	},
	{ "keys": ["ctrl+alt+shift+m"], "command": "new_pane" },
	{ "keys": ["ctrl+alt+shift+u"], "command": "close_pane" },
	{ "keys": ["ctrl+alt+shift+left"], "command": "focus_neighboring_group", "args": {"forward": false} },
	{ "keys": ["ctrl+alt+shift+right"], "command": "focus_neighboring_group" },
	
	{ "keys": ["ctrl+alt+shift+="], "command": "increase_font_size" },
	{ "keys": ["ctrl+alt+shift+-"], "command": "decrease_font_size" },

]
